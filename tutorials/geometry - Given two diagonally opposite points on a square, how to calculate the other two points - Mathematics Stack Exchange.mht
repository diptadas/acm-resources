<pre><code>  x1 =3D ?  ;  y1 =3D ? ;    // First diagonal point
  x2 =3D ?  ;  y2 =3D ? ;    // Second diagonal point

  xc =3D (x1 + x2)/2  ;  yc =3D (y1 + y2)/2  ;    // Center point
  xd =3D (x1 - x2)/2  ;  yd =3D (y1 - y2)/2  ;    // Half-diagonal

  x3 =3D xc - yd  ;  y3 =3D yc + xd;    // Third corner
  x4 =3D xc + yd  ;  y4 =3D yc - xd;    // Fourth corner
